# DNS database file for domain out.
# It also includes a pointer to the primary server
# of the smaller.example.com subdomain

# [dataBase] -> {
#     domain : {
#         data : [data]
#         domainLog : logs from this domain
#         isSP : boolean
#         (*)ttl : validade da informação --> apenas se for SS
#     }
# }

# [data] -> {domain {type { value, ttl, priority }}
# [domain] [type] [value] [ttl] [priority]

@ DEFAULT out.
TTL DEFAULT 86400

@ SOASP ns1.out. TTL
@ SOAADMIN dns\.admin.out. TTL
@ SOASERIAL 0130122022 TTL
@ SOAREFRESH 14400 TTL
@ SOARETRY 3600 TTL
@ SOAEXPIRE 604800 TTL

@ NS ns1.out. TTL
@ NS ns2.out. TTL
@ NS ns3.out. TTL

in.@ NS ns1.in.out. TTL
in.@ NS ns2.in.out. TTL
in.@ NS ns3.in.out. TTL

@ MX mx1.out. TTL 10
@ MX mx2.out. TTL 20

ns1 A 10.0.16.10 TTL
ns2 A 10.0.17.10 TTL
ns3 A 10.0.16.16 TTL
ns1.in A 10.0.18.10 TTL
ns2.in A 10.0.19.10 TTL
ns3.in A 10.0.18.16 TTL
mx1 A 10.0.16.11 TTL
mx2 A 10.0.16.12 TTL
www1 A 10.0.16.14 TTL 200
www2 A 10.0.16.15 TTL 200
ftp A 10.0.16.13 TTL

sp CNAME ns1 TTL
ss1 CNAME ns2 TTL
ss2 CNAME ns3 TTL
mail1 CNAME mx1 TTL
mail2 CNAME mx2 TTL