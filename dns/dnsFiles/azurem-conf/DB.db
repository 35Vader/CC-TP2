# DNS database file for domain azurem.uni.
# It also includes a pointer to the primary server
# of the smaller.example.com subdomain

# [dataBase] -> {
#     domain : {
#         data : [data]
#         domainLog : logs from this domain
#         isSP : boolean
#         (*)ttl : validade da informação --> apenas se for SS
#     }
# }

# [data] -> {domain {type { value, ttl, priority }}
# [domain] [type] [value] [ttl] [priority]

@ DEFAULT azurem.uni.
TTL DEFAULT 86400

@ SOASP ns1.azurem.uni. TTL
@ SOAADMIN dns\.admin.azurem.uni. TTL
@ SOASERIAL 0130122022 TTL
@ SOAREFRESH 14400 TTL
@ SOARETRY 3600 TTL
@ SOAEXPIRE 604800 TTL

@ NS ns1.azurem.uni. TTL
@ NS ns2.azurem.uni. TTL
@ NS ns3.azurem.uni. TTL

mec.@ NS ns1.mec.azurem.uni. TTL
mec.@ NS ns2.mec.azurem.uni. TTL
mec.@ NS ns3.mec.azurem.uni. TTL

@ MX mx1.azurem.uni. TTL 10
@ MX mx2.azurem.uni. TTL 20

ns1 A 10.0.16.10 TTL
ns2 A 10.0.17.10 TTL
ns3 A 10.0.16.16 TTL
ns1.mec A 10.0.18.10 TTL
ns2.mec A 10.0.19.10 TTL
ns3.mec A 10.0.18.16 TTL
mx1 A 10.0.16.11 TTL
mx2 A 10.0.16.12 TTL
www1 A 10.0.16.14 TTL 200
www2 A 10.0.16.15 TTL 200
ftp A 10.0.16.13 TTL

sp CNAME ns1 TTL
ss1 CNAME ns2 TTL
ss2 CNAME ns3 TTL
mail1 CNAME mx1 TTL
mail2 CNAME mx2 TTL